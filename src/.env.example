# Database
DB_URL=jdbc:postgresql://localhost:5432/dbname
DB_USERNAME=db_username
DB_PASSWORD=db_password

# JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRATION=86400000

# Security
SECURITY_USER=admin
SECURITY_PASSWORD=admin_password

# Server
SERVER_PORT=8080

# JPA
JPA_DDL_AUTO=update
SHOW_SQL=false
FORMAT_SQL=false

# Logging
LOG_LEVEL=INFO
SPRING_LOG_LEVEL=INFO
HIBERNATE_LOG_LEVEL=INFO 